{
  "name": "dark-please",
  "displayName": "Dark, please!",
  "description": "A pleasant-looking low-contrast dark theme.",
  "version": "1.1.15",
  "icon": "assets/img/dark-please-logo.png",
  "author": {
    "name": "Nick Eoneof"
  },
  "publisher": "eoneof",
  "homepage": "https://github.com/eoneof/dark-please-vscode-theme",
  "repository": {
    "type": "git",
    "url": "https://github.com/eoneof/dark-please-vscode-theme"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "semantic highlighting",
    "syntax highlighting",
    "dark",
    "light",
    "color",
    "low contrast",
    "TypeScript",
    "JavaScript",
    "CSS",
    "HTML",
    "frontend"
  ],
  "scripts": {
    "build": "npm run convert && npm run generate",
    "convert": "node ./src/converter/convert.js",
    "generate": "node ./src/generator/generate.js",
    "package": "vsce package -o ./dist",
    "release": "npm run convert && npm run generate && npm run package"
  },
  "contributes": {
    "grammars": [
      {
        "scopeName": "keyword.className.react.js",
        "path": "./syntaxes/classname.injection.json",
        "injectTo": [
          "source.js",
          "source.jsx"
        ]
      },
      {
        "scopeName": "inject.promise",
        "path": "./syntaxes/promise.injection.json",
        "injectTo": [
          "source.js",
          "source.jsx"
        ]
      },
      {
        "scopeName": "props.react.js",
        "path": "./syntaxes/props.injection.json",
        "injectTo": [
          "source.js",
          "source.jsx"
        ]
      }
    ],
    "themes": [
      {
        "id": "dark-please",
        "label": "Dark, please!",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-please-vscode-theme.json"
      },
      {
        "id": "light-please",
        "label": "Light, please!",
        "uiTheme": "vs",
        "path": "./themes/light-please-vscode-theme.json"
      }
    ],
    "semanticTokenScopes": []
  },
  "devDependencies": {
    "@types/node": "^16.9.1",
    "ase-util": "^1.0.3",
    "jsdom": "^19.0.0",
    "nodemon": "^2.0.16",
    "tinycolor2": "^1.4.2"
  }
}